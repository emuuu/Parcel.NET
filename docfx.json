{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/**/*.csproj"],
          "src": "."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["docs/**.md", "docs/**/toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["icon.png"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "noLangKeyword": false
  }
}
